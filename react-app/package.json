{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "react-webpack environment",
  "main": "index.js",
  "author": "Mark_Zhang",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.3.2",
    "history": "^4.9.0",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jsx-loader": "^0.13.2",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "opener": "^1.5.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.0",
    "react-hot-loader": "^4.8.3",
    "react-loadable": "^5.5.0",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^7.1.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-stylish": "^0.1.8"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist/",
      "yarn.lock"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "linc": "node_modules/.bin/standard",
    "fix": "node_modules/.bin/standard --fix",
    "start": "cross-env NODE_ENV=development PORT=4500 node build/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack --progress --colors --config ./build/webpack.config.production.js",
    "build:online": "cross-env NODE_ENV=production_online node_modules/.bin/webpack --progress --colors --config ./build/webpack.config.production.js",
    "precommit": "lint-staged"
  }
}
